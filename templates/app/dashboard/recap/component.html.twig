<div class="col-lg-12" {{ attributes }}>
    <div class="card">
        <div class="card-body">
            {{ form_start(form) }}
            <div class="row">
                <div class="col-lg-2">
                    {{ form_label(form.date_begin)}}
                    {{ form_widget(form.date_begin)}}
                </div>
                <div class="col-lg-2">
                    {{ form_label(form.date_end)}}
                    {{ form_widget(form.date_end)}}
                </div>
                <div class="col-lg-6">
                    {{ form_label(form.project)}}
                    {{ form_widget(form.project)}}
                </div>
                <div class="col-lg-2">
                    {{ form_label(form.type)}}
                    {{ form_widget(form.type)}}
                </div>
            </div>
            <div class="col-lg-12 d-flex justify-content-start">
                <div class="row mt-3">
                    <label class="form-label">Sales</label>
                    {% for user in form.user %}
                        <div class="col-2">
                            {{ form_label(user)}}
                            {{ form_widget(user, {'attr': {'data-model': 'user'}}) }}
                        </div>
                    {% endfor %}

                </div>
            </div>
            {{ form_end(form) }}
        </div>
    </div>
    <div class="card">
        <div class="card-body">
            <table class="table">
                <thead>
                <tr>
                    <th scope="col">Produit</th>
                    <th scope="col">Client</th>
                    <th scope="col">Date</th>
                    <th scope="col">Prix unitaire</th>
                    <th scope="col">Prix total</th>
                    <th scope="col">Com TF</th>
                    <th scope="col">Com Sales</th>
                    <th scope="col">Valid√©e ?</th>
                    <th scope="col"></th>
                </tr>
                </thead>
                <tbody>
                    {% for sale in this.sales %}

                    {% endfor %}
                </tbody>
            </table>
        </div>
    </div>
</div>