{
    "name": "bastien70/symfony_6_skeleton",
    "description": "Description of the project",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.11",
        "dompdf/dompdf": "^2.0",
        "easycorp/easyadmin-bundle": "^4.4",
        "endroid/qr-code": "^4.6",
        "gedmo/doctrine-extensions": "^3.5",
        "knplabs/knp-paginator-bundle": "*",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpoffice/phpspreadsheet": "^1.29",
        "proglab/monolog-discord-handler": "^1.0",
        "scheb/2fa-bundle": "^6.4",
        "scheb/2fa-totp": "^6.4",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "6.4.*",
        "symfony/console": "6.4.*",
        "symfony/discord-notifier": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/expression-language": "6.4.*",
        "symfony/finder": "6.4.*",
        "symfony/flex": "^2",
        "symfony/form": "6.4.*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/html-sanitizer": "6.4.*",
        "symfony/http-client": "6.4.*",
        "symfony/intl": "6.4.*",
        "symfony/lock": "6.4.*",
        "symfony/mailer": "6.4.*",
        "symfony/mailjet-mailer": "6.4.*",
        "symfony/messenger": "6.4.*",
        "symfony/mime": "6.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "6.4.*",
        "symfony/process": "6.4.*",
        "symfony/property-access": "6.4.*",
        "symfony/property-info": "6.4.*",
        "symfony/proxy-manager-bridge": "6.4.*",
        "symfony/rate-limiter": "6.4.*",
        "symfony/runtime": "6.4.*",
        "symfony/security-bundle": "6.4.*",
        "symfony/serializer": "6.4.*",
        "symfony/string": "6.4.*",
        "symfony/translation": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/ux-chartjs": "^2.7",
        "symfony/ux-live-component": "^2.16",
        "symfony/validator": "6.4.*",
        "symfony/web-link": "6.4.*",
        "symfony/webpack-encore-bundle": "^1.16",
        "symfony/yaml": "6.4.*",
        "symfonycasts/dynamic-forms": "^0.1.1",
        "symfonycasts/reset-password-bundle": "^1.11",
        "symfonycasts/verify-email-bundle": "^1.7",
        "twig/cache-extra": "^3.3",
        "twig/cssinliner-extra": "^3.3",
        "twig/extra-bundle": "^3.3",
        "twig/inky-extra": "^3.3",
        "twig/string-extra": "^3.7",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^7.1",
        "dbrekelmans/bdi": "^1.0",
        "deployer/deployer": "^7.1",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "icanhazstring/composer-unused": "^0.8.7",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/debug-bundle": "6.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/panther": "^2.0",
        "symfony/phpunit-bridge": "^6.4",
        "symfony/stopwatch": "6.4.*",
        "symfony/web-profiler-bundle": "6.4.*",
        "zenstruck/foundry": "^1.24"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "ci": [
            "docker run --init --rm -v \"%CD%:/project\" -w /project jakzal/phpqa:php8.1-alpine php-cs-fixer fix --config=\"./.php-cs-fixer.dist.php\" --verbose --using-cache=no",
            "docker run --init --rm -v \"%CD%:/project\" -w /project jakzal/phpqa phpstan analyse src"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    }
}
